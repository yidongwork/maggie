package vehicle;

public class Wheelchair extends Vehicle{
	public Wheelchair(){
		this.Vehicle_Type = "Wheelchair";
	}
	
	public int getNumofWheels(){
		return 2;
	}
	
	public static void main(String[]args){
		Vehicle myVehicle = new Wheelchair();
		System.out.println("My " + myVehicle.getVehicleType() + " has " + myVehicle.getNumOfWheels() + " Seats. ");
		
		
	}
	
}

